  "scripts": {
    "start": "expo start",
    "dev": "run-p web dev:desktop",
    "dev:desktop": "pushd desktop && yarn dev",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:copy-desktop": "shx cp -R ./web-build ./desktop/build",
    "compile:web": "expo build:web",
    "compile:android": "expo build:android",
    "compile:ios": "expo build:ios",
    "compile:desktop": "cd desktop && yarn compile",
    "compile:desktop:ci": "cd desktop && yarn compile:ci",
    "pack:web": "run-s compile:web now",
    "pack:desktop": "run-s compile:web web:copy-desktop compile:desktop",
    "pack:desktop:ci": "run-s web:copy-desktop compile:desktop:ci",
    "now": "now -A ./now.json --token=$NOW_TOKEN ./web-build ",
    "eject": "expo eject",
    "prepare": "patch-package",
    "git:clear-branch": "git branch | grep -v 'master' | xargs git branch -D"
  },
